<%= form_for @job do |f| %>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.label :origin %>
<%= f.select :origin, ['Alberta', 'California', 'Florida', 'New York', 'Seoul', 'Texas' 'Tokyo', 'Vancouver'] %>
<%= f.label :destination %>
<%= f.select :destination, ['Alberta', 'California', 'Florida', 'New York', 'Seoul', 'Texas', 'Tokyo', 'Vancouver'] %>
<%= f.label :cost %>
<%= f.text_field :cost %>
<%= f.label :containers_needed %>
<%= f.number_field :containers_needed %>
<%= f.submit %>
<% end %>
